<launch>
  <node pkg="xqserial_server" type="xqserial_server" name="motor_driver" output="screen">
    <param name="debug_flag" value="true"/>
    <param name="wheel_separation" value="0.360"/>
    <param name="port_car" value="/dev/ttyUSB002"/>
    <param name="port_imu" value="/dev/ttyUSB001"/>
    <!-- <param name="port" value="/dev/pts/5"/> -->

  </node>
  <node pkg="tf" type="static_transform_publisher" name="baselink_broadcaster" args="0 0 0.15 0 0 0 1 base_footprint base_link 50">
  </node>
</launch>
